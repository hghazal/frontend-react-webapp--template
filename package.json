{
  "name": "app.roller.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "better-npm-run dev",
    "start": "better-npm-run prod",
    "build": "better-npm-run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "dev": {
      "command": "concurrently \"node webpack/dev.entry\" \"node src/server\"",
      "env": {
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": 3000
      }
    },
    "prod": {
      "command": "node src/server",
      "env": {
        "NODE_ENV": "production",
        "HOST": "localhost",
        "PORT": 5000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.entry.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "",
  "license": "ISC",
